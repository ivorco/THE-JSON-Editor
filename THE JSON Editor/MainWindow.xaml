<Window x:Class="THE_JSON_Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:THE_JSON_Editor"
        mc:Ignorable="d"
        Title="THE JSON Editor" Height="450" Width="800" WindowStartupLocation="CenterScreen">

    <Window.DataContext>
        <local:ComplexValue/>
    </Window.DataContext>

    <TreeView ItemsSource="{Binding ComplexValues}">
        <TreeView.ItemTemplate>
            <HierarchicalDataTemplate ItemsSource="{Binding ComplexValues}">
                <Label Content="{Binding Name}"/>
            </HierarchicalDataTemplate>
        </TreeView.ItemTemplate>
    </TreeView>
</Window>
